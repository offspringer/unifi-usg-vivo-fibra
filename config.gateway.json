                        }
                    }
                },
                "200.161.71.48/31": {
                    "next-hop": {
                        "10.5.160.1": {
                            "description": "IPTV",
                            "distance": "1"
                        }
                    }
                },
                "201.0.52.0/23": {
                    "next-hop": {
                        "10.5.160.1": {
                            "description": "IPTV",
                            "distance": "1"
                        }
                    }
                }
