{
    "firewall": {
        "group": {
            "address-group": {
                "5ef91eccb9d5df03bc6f4aec": {
                    "address": [
                        "172.28.0.0/14",
                        "201.0.52.0/23",
                        "200.161.71.0/24",
                        "177.16.0.0/16"
                    ],
                    "description": "customized-IPTV"
                },
                "5ef924fab9d5df03bc6f4b01": {
                    "address": [
                        "239.0.0.0/8"
                    ],
                    "description": "customized-IPTV Multicast"
                }
            }
        },
        "name": {
            "WAN_IN": {
                "default-action": "drop",
                "description": "packets from internet to intranet",
                "rule": {
                    "2000": {
                        "action": "accept",
                        "description": "allow IPTV Multicast UDP",
                        "destination": {
                            "group": {
                                "address-group": "5ef924fab9d5df03bc6f4b01"
                            }
                        },
                        "protocol": "udp",
                        "source": {
                            "group": {
                                "address-group": "5ef91eccb9d5df03bc6f4aec"
                            }
                        }
                    },
                    "2001": {
                        "action": "accept",
                        "description": "allow IGMP",
                        "protocol": "igmp"
                    }
                }
            },
            "WAN_LOCAL": {
                "default-action": "drop",
                "description": "packets from internet to gateway",
                "rule": {
                    "2000": {
                        "action": "accept",
                        "description": "allow IPTV Multicast UDP",
                        "destination": {
                            "group": {
                                "address-group": "5ef924fab9d5df03bc6f4b01"
                            }
                        },
                        "protocol": "udp",
                        "source": {
                            "group": {
                                "address-group": "5ef91eccb9d5df03bc6f4aec"
                            }
                        }
                    },
                    "2001": {
                        "action": "accept",
                        "description": "allow IGMP",
                        "protocol": "igmp"
                    },
                    "2002": {
                        "action": "accept",
                        "description": "allow ICMP",
                        "icmp": {
                            "type-name": "any"
                        },
                        "protocol": "icmp"
                    }
                }
            }
        }
    },
    "interfaces": {
        "ethernet": {
            "eth0": {
                "description": "WAN",
                "vif": {
                    "10": {
                        "description": "Internet",
                        "dhcp-options": {
                            "default-route": "no-update",
                            "default-route-distance": "1",
                            "name-server": "update"
                        },
                        "pppoe": {
                            "2": {
                                "description": "Internet (PPPoE)",
                                "default-route": "force",
                                "firewall": {
                                    "in": {
                                        "ipv6-name": "WANv6_IN",
                                        "name": "WAN_IN"
                                    },
                                    "local": {
                                        "ipv6-name": "WANv6_LOCAL",
                                        "name": "WAN_LOCAL"
                                    },
                                    "out": {
                                        "ipv6-name": "WANv6_OUT",
                                        "name": "WAN_OUT"
                                    }
                                },
                                "mtu": "1492",
                                "name-server": "none",
                                "password": "cliente",
                                "user-id": "cliente@cliente"
                            }
                        }
                    },
                    "20": {
                        "description": "IPTV",
                        "address": [
                            "dhcp"
                        ],
                        "dhcp-options": {
                            "default-route": "no-update",
                            "default-route-distance": "100",
                            "name-server": "update"
                        },
                        "firewall": {
                            "in": {
                                "ipv6-name": "WANv6_IN",
                                "name": "WAN_IN"
                            },
                            "local": {
                                "ipv6-name": "WANv6_LOCAL",
                                "name": "WAN_LOCAL"
                            },
                            "out": {
                                "ipv6-name": "WANv6_OUT",
                                "name": "WAN_OUT"
                            }
                        },
                        "ip": {
                            "source-validation": "loose"
                        }
                    },
                    "30": {
                        "description": "VoIP",
                        "address": [
                            "dhcp"
                        ],
                        "dhcp-options": {
                            "default-route": "no-update",
                            "default-route-distance": "200",
                            "name-server": "update"
                        },
                        "firewall": {
                            "in": {
                                "ipv6-name": "WANv6_IN",
                                "name": "WAN_IN"
                            },
                            "local": {
                                "ipv6-name": "WANv6_LOCAL",
                                "name": "WAN_LOCAL"
                            },
                            "out": {
                                "ipv6-name": "WANv6_OUT",
                                "name": "WAN_OUT"
                            }
                        },
                        "ip": {
                            "source-validation": "loose"
                        }
                    }
                }
            }
        }
    },
    "port-forward": {
        "wan-interface": "pppoe2"
    },
    "protocols": {
        "igmp-proxy": {
            "disable-quickleave": "''",
            "interface": {
                "eth0.10": {
