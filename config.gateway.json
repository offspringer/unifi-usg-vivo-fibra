        }
    },
    "service": {
        "dns": {
            "forwarding": {
                "except-interface": [
                    "pppoe2"
                ]
            }
        },
        "nat": {
            "rule": {
                "5000": {
                    "description": "MASQ corporate_network to IPTV",
                    "log": "disable",
                    "outbound-interface": "eth0.20",
                    "protocol": "all",
                    "type": "masquerade"
                },
                "5001": {
                    "description": "MASQ corporate_network to VoIP",
                    "log": "disable",
